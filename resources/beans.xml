<?xml version="1.0" encoding="UTF-8"?>
<beans>
	<bean id="basebaseservice" class="com.ant.test.BaseBaseService">
	    <property type="com.ant.test.AServiceImpl" name="as" ref="aservice"/>
	</bean>
	<bean id="aservice" class="com.ant.test.AServiceImpl">
		<constructor-arg type="String" name="name" value="abc"/>
		<constructor-arg type="int" name="level" value="3"/>
        <property type="String" name="property1" value="Someone says"/>
        <property type="String" name="property2" value="Hello World!"/>
        <property type="com.minis.test.BaseService" name="ref1" ref="baseservice"/>
	</bean>
	<bean id="baseservice" class="com.ant.test.BaseService">
	        <property type="com.ant.test.BaseBaseService" name="bbs" ref="basebaseservice"/>
	</bean>

	
</beans>